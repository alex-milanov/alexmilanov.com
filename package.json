{
  "name": "alexmilanov.com",
  "version": "0.2.0",
  "description": "my personal site",
  "main": "gulpfile.js",
  "scripts": {
    "prebuild": "node bin/move-assets.js",
    "build": "npm run build:js && npm run build:sass",
    "build:js": "browserify src/js/index.js -o dist/js/app.js --debug",
    "build:sass": "node-sass --include-path=$(node bin/sass-paths.js) src/sass/style.sass dist/css/style.css",
    "build:legacy": "browserify -t [ babelify --global --presets [ es2015 ] --plugins transform-object-assign ] src/js/index.js -o dist/js/app.js --debug",
    "watch": "npm run watch:js & npm run watch:sass & npm run livereload",
    "watch:js": "watchify -p browserify-hmr src/js/index.js -o dist/js/app.js --debug",
    "watch:sass": "npm run build:sass -- --watch src/sass/**/*",
    "livereload": "livereload dist/**/*.css -d",
    "start": "npm run build && serve --path dist & npm run watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-hmr": "^0.3.5",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.6.0",
    "fs-extra": "^2.1.2",
    "livereload": "^0.6.2",
    "node-sass": "^4.5.2",
    "node-serve": "0.0.3",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "bourbon": "^4.3.3",
    "bourbon-neat": "^2.0.0",
    "font-awesome": "^4.7.0",
    "iblokz-data": "^1.0.2",
    "iblokz-snabbdom-helpers": "^1.0.0",
    "moment": "^2.18.1",
    "rx": "^4.1.0",
    "snabbdom": "^0.6.7",
    "superagent": "^3.5.2"
  }
}
